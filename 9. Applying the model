# Single-player salary prediction

# Include Player in the single_player data
single_player <- Final_data %>%
  filter(Player == "Terance Mann") %>%
  select(Player, PTS, Age, MP, `3P`, STL, TRB, AST)  # Include Player

# Predict salary using rf_model
predicted_salary <- predict(rf_model, newdata = single_player)

# Print the predicted salary
cat("Predicted Salary for Terance Mann:", predicted_salary, "\n")

